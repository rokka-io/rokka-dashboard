{
  "properties": {
    "basestack": { "type": ["string", "null"], "default": null, "pattern": "[a-z0-9\\-_]+" },
    "jpg.quality": { "type": "integer", "minimum": 1, "maximum": 100, "default": 80 },
    "webp.quality": { "type": "integer", "minimum": 1, "maximum": 100, "default": 80 },
    "heif.quality": { "type": "integer", "minimum": 1, "maximum": 100, "default": 40 },
    "av1.quality": { "type": "integer", "minimum": 1, "maximum": 100, "default": 50 },
    "gif.quality": { "type": "integer", "minimum": 1, "maximum": 100, "default": 60 },
    "jp2.quality": { "type": "integer", "minimum": 1, "maximum": 100, "default": 40 },
    "optim.quality": { "type": "integer", "minimum": 0, "maximum": 10, "default": 4 },
    "png.compression_level": { "type": "integer", "minimum": 0, "maximum": 9, "default": 7 },
    "dpr": { "type": "number", "minimum": 1, "maximum": 10, "default": 1 },
